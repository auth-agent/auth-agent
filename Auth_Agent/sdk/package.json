{
  "name": "auth-agent-sdk",
  "version": "0.0.3",
  "description": "Official SDK for Auth Agent - OAuth 2.1 for websites and AI agents",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    },
    "./client/react": {
      "types": "./dist/client/react.d.ts",
      "import": "./dist/client/react.mjs",
      "require": "./dist/client/react.js"
    },
    "./agent": {
      "types": "./dist/agent.d.ts",
      "import": "./dist/agent.mjs",
      "require": "./dist/agent.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "auth",
    "oauth",
    "oauth2",
    "authentication",
    "ai-agent",
    "pkce",
    "oidc",
    "auth-agent"
  ],
  "author": "Auth Agent Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/auth-agent/auth-agent.git",
    "directory": "Auth_Agent/sdk"
  },
  "homepage": "https://docs.auth-agent.com",
  "bugs": {
    "url": "https://github.com/auth-agent/auth-agent/issues"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@vitest/coverage-v8": "^1.0.0",
    "jsdom": "^23.2.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
